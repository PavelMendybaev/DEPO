<Page x:Class="WpfApp37.Pages.Defect"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp37.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Defect">

    <Grid>
        <DataGrid AutoGenerateColumns="False" x:Name="dg1">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Название продукта" Binding="{ Binding NameProduct}"></DataGridTextColumn>
                <DataGridTextColumn Header="Номер хранения" Binding="{ Binding NumberStore}"></DataGridTextColumn>
                <DataGridTextColumn Header="Упадок" Binding="{ Binding Breakdown}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
